{
  "name": "@cartesi/decoder",
  "version": "0.1.0",
  "description": "",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "scripts": {
    "build": "dotenv -- tsup \"src/**/*.ts*\" --format esm,cjs --dts",
    "dev": "dotenv -- tsup \"src/**/*.ts*\" --format esm,cjs --dts --watch",
    "lint": "eslint \"src/**/*.ts*\"",
    "format:check": "prettier \"**/*.{ts,tsx}\" --check",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "test:watch": "vitest",
    "test:ci": "vitest run --coverage"
  },
  "dependencies": {
    "@shazow/whatsabi": "0.11.0",
    "dotenv": "^16.4.5",
    "ethers": "^6.9.0",
    "viem": "^2",
    "wagmi": "^2"
  },
  "devDependencies": {
    "@cartesi/tsconfig": "*",
    "dotenv-cli": "latest",
    "eslint": "^8",
    "eslint-config-cartesi": "*",
    "tsup": "^7",
    "typescript": "^5",
    "vitest": "^1.6.0"
  }
}