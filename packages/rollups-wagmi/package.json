{
    "name": "@cartesi/rollups-wagmi",
    "version": "0.0.0",
    "main": "./dist/index.cjs",
    "module": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "type": "module",
    "files": [
        "dist"
