{
    "name": "@cartesi/rollups-explorer-ui",
    "version": "0.0.0",
    "main": "./dist/index.js",
    "module": "./dist/index.mjs",
    "types": "./dist/index.d.ts",
    "sideEffects": false,
    "license": "Apache-2.0",
    "files": [
        "dist/**"
    ],
    "scripts": {
        "build": "tsup src/index.tsx --format esm,cjs --dts --external react",
        "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist",
