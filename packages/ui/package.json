        "lint": "eslint \"src/**/*.ts*\""
    },
    "dependencies": {},
    "devDependencies": {
        "@cartesi/tsconfig": "workspace:*",
        "@types/react": "^18",
        "@types/react-dom": "^18",
        "eslint": "^8",
        "eslint-config-cartesi": "workspace:*",
        "react": "^18",
        "tsup": "^5",
        "typescript": "^5"
    },
    "publishConfig": {
        "access": "public"
    }
}
